{
  "name": "@atlasp/backend",
  "version": "1.8.3",
  "description": "Atlasp's backend",
  "main": "index.js",
  "repository": "https://github.com/kadhirvelm/atlasP-backend.git",
  "author": "kadhirvelm",
  "license": "MIT",
  "dependencies": {
    "@types/google-libphonenumber": "^7.4.17",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cron": "^1.4.1",
    "express": "^4.16.3",
    "google-auth-library": "^2.0.0",
    "google-libphonenumber": "^3.1.15",
    "googleapis": "27",
    "jsonwebtoken": "^8.3.0",
    "mongodb": "^3.1.3",
    "nodemailer": "^4.6.8",
    "nodemailer-ses-transport": "^1.5.1"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch & nodemon dist",
    "server": "tsc && node dist",
    "lint": "eslint src --ext ts && tslint src/**/*",
    "lint:fix": "prettier src/**/* --write && eslint src --ext ts --fix && tslint src/**/* --fix",
    "test": "mocha -r ts-node/register src/**/__tests__/*.spec.ts --exit",
    "test:coverage": "nyc mocha -r ts-node/register -r source-map-support/register --recursive src/**/__tests__/*.spec.ts --exit",
    "test:watch": "mocha -r ts-node/register --watch-extensions spec.ts,ts src/**/__tests__/*.spec.ts --watch"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "*.js"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.6",
    "@types/cron": "^1.3.0",
    "@types/eslint": "^4.16.3",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.4",
    "@types/mongodb-memory-server": "^1.8.0",
    "@types/node": "^10.7.0",
    "@types/nodemailer": "^4.6.5",
    "@types/nodemailer-ses-transport": "^3.1.4",
    "@types/supertest": "^2.0.6",
    "chai": "^4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mongodb-memory-server": "^2.4.3",
    "nodemon": "^1.18.3",
    "nyc": "^13.0.1",
    "prettier": "1.14.2",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
